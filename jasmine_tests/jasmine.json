{
    "spec_dir": "specs",
    "spec_files": [
        "*_spec.js"
    ],
    "helpers": [
        "../helpers/*_helper.js"
    ],
    "stopSpecOnExpectationFailure": true,
    "random": false,
    "child_processes": 3,
    "junitReporterOpts": {
        "savePath": "/testresults",
        "consolidate": true,
        "useDotNotation": true,
        "filePrefix": "test-",
        "consolidateAll": false 
    }
}
